    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.6",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
